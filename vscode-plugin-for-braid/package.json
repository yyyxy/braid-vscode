{
	"name": "vscode-plugin-test",
	"displayName": "vscode-plugin-test",
	"description": "代码推荐",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Code Recommendation",
			"properties": {
				"vscodePluginDemo.yourName": {
					"type": "string",
					"default": "guest",
					"description": "User ID"
				},
				"vscodePluginDemo.groupName": {
					"type": "string",
					"default": "oneteam",
					"description": "Group ID"
				},
				"vscodePluginDemo.showTip": {
					"type": "boolean",
					"default": false,
					"description": "Start Reconmmendation"
				}
			}
		},
		"commands": [
			{
				"command": "extension.query",
				"title": "Input your query"
			}
		],
		"keybindings": [
			{
				"command": "extension.query",
				"key": "ctrl+f9",
				"mac": "cmd+f9",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.query",
					"group": "navigation"
				}
			],
			"explorer/context": [
				{
					"command": "extension.cswebview",
					"group": "navigation"
				},
				{
					"command": "extension.apiwebview",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"iview": "^3.2.2"
	}
}
